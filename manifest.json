{
    "manifest_version": 2,
    "name": "Extension in development",
    "version": "0.6.0",
    "description": "Send history of visited pages ...",
    "background": {
        "scripts": [
            "dist/bundle.js"
        ]
    },
    "permissions": [
        "history",
        "notifications",
        "geolocation",
        "storage",
        "alarms"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}